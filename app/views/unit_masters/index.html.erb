<h1>Listing unit_masters</h1>

<table>
  <thead>
    <tr>
      <th>Unit name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @unit_masters.each do |unit_master| %>
    <tr>
      <td><%= unit_master.unit_name %></td>
      <td><%= link_to 'Show', unit_master %></td>
      <td><%= link_to 'Edit', edit_unit_master_path(unit_master) %></td>
      <td><%= link_to 'Destroy', unit_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Unit master', new_unit_master_path %>
